{
  "name": "angular-test",
  "version": "0.0.6",
  "scripts": {
    "ng": "nx",
    "start": "npm run angular-test.art && ng serve",
    "build": "npm run angular-test.art && ng build",
    "build:prod": "npm run angular-test.art && ng build --progress --prod",
    "test": "npm run angular-test.art && ng test",
    "lint": "npm run angular-test.art && nx workspace-lint && ng lint",
    "e2e": "npm run angular-test.art && ng e2e",
    "nx": "nx",
    "affected:apps": "nx affected:apps",
    "affected:libs": "nx affected:libs",
    "affected:build": "nx affected:build",
    "affected:e2e": "nx affected:e2e",
    "affected:test": "nx affected:test",
    "affected:lint": "nx affected:lint",
    "affected:dep-graph": "nx affected:dep-graph",
    "affected": "nx affected",
    "format": "nx format:write",
    "format:write": "nx format:write",
    "format:check": "nx format:check",
    "update": "ng update @nrwl/workspace",
    "update:check": "ng update",
    "dep-graph": "nx dep-graph",
    "workspace-schematic": "nx workspace-schematic",
    "help": "nx help",
    "postinstall": "node ./decorate-angular-cli.js && ngcc --properties es2015 browser module main --first-only --create-ivy-entry-points",
    "angular-test.art": "node apps/angular-test/src/angular-test-ascii-art.js",
    "commit": "npx git-cz",
    "release": "standard-version",
    "analyze": "yarn build --stats-json && webpack-bundle-analyzer ./dist/apps/angular-test/stats.json"
  },
  "husky": {
    "hooks": {
      "pre-push": "npm run build:prod"
    }
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^11.0.7",
    "@angular/cdk": "^11.0.3",
    "@angular/common": "^11.0.7",
    "@angular/compiler": "^11.0.7",
    "@angular/core": "^11.0.7",
    "@angular/flex-layout": "^11.0.0-beta.33",
    "@angular/forms": "^11.0.7",
    "@angular/material": "^11.0.3",
    "@angular/platform-browser": "^11.0.7",
    "@angular/platform-browser-dynamic": "^11.0.7",
    "@angular/router": "^11.0.7",
    "@ngx-translate/core": "^13.0.0",
    "@ngx-translate/http-loader": "^6.0.0",
    "@nrwl/angular": "11.1.1",
    "@nrwl/workspace": "^11.1.1",
    "core-js": "^2.5.4",
    "json2typescript": "^1.4.1",
    "ngx-quicklink": "^0.2.6",
    "rxjs": "^6.6.3",
    "tslib": "^2.0.0",
    "zone.js": "~0.11.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.1100.6",
    "@angular/cli": "~11.0.6",
    "@angular/compiler-cli": "^11.0.7",
    "@angular/language-service": "^11.0.7",
    "@nrwl/jest": "11.1.1",
    "@nrwl/workspace": "11.1.1",
    "@types/jasmine": "~2.8.6",
    "@types/jasminewd2": "~2.0.3",
    "@types/jest": "26.0.8",
    "@types/node": "~8.9.4",
    "codelyzer": "6.0.1",
    "commitizen": "^4.2.2",
    "cz-conventional-changelog": "^3.3.0",
    "husky": "^3.1.0",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "jest": "26.2.2",
    "jest-preset-angular": "8.3.1",
    "karma": "~1.7.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.0",
    "karma-jasmine": "~1.1.1",
    "karma-jasmine-html-reporter": "^0.2.2",
    "prettier": "2.1.2",
    "protractor": "~5.3.0",
    "standard-version": "^9.1.0",
    "ts-jest": "26.4.0",
    "ts-node": "9.1.0",
    "tslint": "6.1.3",
    "typescript": "~4.0.5",
    "webpack-bundle-analyzer": "^4.3.0"
  },
  "config": {
    "commitizen": {
      "path": "./angular-test/node_modules/cz-conventional-changelog"
    }
  }
}
